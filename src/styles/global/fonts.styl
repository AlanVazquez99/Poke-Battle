/* jetbrains-mono-regular - latin */
@font-face 
  font-family: 'JetBrains';
  font-style: normal;
  font-weight: 400;
  src: url('../../assets/fonts/jetbrains-regular.eot'); /* IE9 Compat Modes */
  src: local(''),
       url('../../assets/fonts/jetbrains-regular.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
       url('../../assets/fonts/jetbrains-regular.woff2') format('woff2'), /* Super Modern Browsers */
       url('../../assets/fonts/jetbrains-regular.woff') format('woff'), /* Modern Browsers */
       url('../../assets/fonts/jetbrains-regular.ttf') format('truetype'), /* Safari, Android, iOS */
       url('../../assets/fonts/jetbrains-regular.svg#JetBrainsMono') format('svg'); /* Legacy iOS */

